enum Status {
	ACTIVE,DEACTIVE, DRAFT
}

enum StaffType{
	TEACHING, NONTEACHING, GUEST
}

enum Gender {
    MALE, FEMALE, OTHER
}

enum Religion{
   HINDU,MUSLIM,CHRISTIAN,SIKH,BUDH
}

enum Caste{
	GENERAL,OBC,SC,ST
}

enum Bloodgroup{
  APOSITIVE,ANEGATIVE, ABPOSITIVE,ABNEGATIVE,OPOSITIVE,ONEGATIVE,BPOSITIVE,BNEGATIVE
}

enum BatchEnum{
	FIRSTYEAR,SECONDYEAR,THIRDYEAR,FOURTHYEAR
}

enum SectionEnum{
	A,B,C,D
}

enum RelationWithStudentEnum{
	FATHER,MOTHER,GUARDIAN
}

enum SubTypeEnum{
	COMMON,ELECTIVE
}

enum Disability{
	YES, NO
}

enum MaritalStatus{
	MARRIED, SINGLE, DIVORCED, WIDOWED
}

entity Country{
	CountryName String 
	CountryCode String 
	IsdCode String
}

entity Currency{
	CurrencyName String 
	CurrencyCode String
	CurrencySymbol String
}

entity State{
	StateName String 
	DivisionType String 
	StateCode String 
}

entity City{
	CityName String 
	CityCode String
	StdCode String
}

entity UserPreference{
	userId String
	academicYearId Long
	collegeId Long
	branchId Long
	departmentId Long
	courseId Long
	semesterId Long
	batchId Long
	sectionId Long
	classId Long
}

entity College {
	collegeName String 
	logoFilePath String
	logoFileName String
	logoFileExtension String
	createdBy String
	createdOn LocalDate
	updatedBy String
	updatedOn LocalDate
	Status String
}

entity Branch{
