	designation String 
	address String 
	email String 
	panNo String 
}

entity BankAccounts{
	bankName String 
	accountNumber String 
	typeOfAccount String 
	ifscCode String 
	branchAddress String 
	corporateId String 
}

entity ExceptionRecord {
	exceptionSource String
	exceptionType String
